{
  "name": "advent-of-code-eslint-checker",
  "version": "1.0.0",
  "description": "Project to run eslint",
  "scripts": {
    "lint": "eslint **/*.js",
    "fix": "eslint --fix **/*.js",
    "format": "npx prettier --write .",
    "check": "eslint . --ext .js && npx prettier --check ."
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7",
    "eslint-config-prettier": "^8",
    "prettier": "2.3.2"
  }
}
